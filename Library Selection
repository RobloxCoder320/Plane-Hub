local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
   Name = "Plane Hub | Library Selection",
   Icon = 0,
   LoadingTitle = "Plane Hub | Library Selection",
   LoadingSubtitle = "By RobloxCoder320",
   Theme = "Default",
})

local Rec = Window:CreateTab("Recommended")
local PC = Window:CreateTab("PC Libraries")
local Mob = Window:CreateTab("Mobile Libraries")

local Button = Rec:CreateButton({
   Name = "Plane Hub | Rayfield",
   Callback = function()
   loadstring(game:HttpGet(("https://raw.githubusercontent.com/RobloxCoder320/Plane-Hub/refs/heads/main/Plane%20Hub%20-%20Rayfield")))()
   Rayfield:Destroy()
   end,
})
