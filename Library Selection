local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Library Selection - Plane Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "Selection"})

local Tab = Window:MakeTab({
	Name = "Recommended",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Plane Hub - Rayfield",
	Callback = function()
        loadstring(game:HttpGet(("https://raw.githubusercontent.com/RobloxCoder320/Plane-Hub/refs/heads/main/Plane%20Hub%20-%20Rayfield")))()
        OrionLib:Destroy()
  	end    
})

local Tab = Window:MakeTab({
	Name = "PC Libraries",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



local Tab = Window:MakeTab({
	Name = "Mobile Libraries",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
