local misc = Misc:CreateSection("Humanoid")

local Slider = Misc:CreateSlider({
   Name = "Walkspeed",
   Range = {16, 250},
   Increment = 1,
   Suffix = "Walkspeed",
   CurrentValue = 16,
   Flag = "WSpeed",
   Callback = function(W)
      game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = W
   end,
})

local Slider = Misc:CreateSlider({
   Name = "Jump Power",
   Range = {50, 500},
   Increment = 1,
   Suffix = "Jump Power",
   CurrentValue = 50,
   Flag = "JSpeed",
   Callback = function(S)
      game.Players.LocalPlayer.Character.Humanoid.JumpPower = S
   end,
})
